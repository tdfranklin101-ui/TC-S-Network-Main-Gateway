#!/usr/bin/env node

/**
 * Simple Health Check Script for Replit Deployments
 * 
 * This file is an executable script that responds to health checks
 * which is what Replit expects for deployments.
 */

console.log('Status: 200 OK');
console.log('Content-Type: application/json');
console.log('');
console.log(JSON.stringify({
  status: 'ok',
  timestamp: new Date().toISOString(),
  message: 'The Current-See health check passed'
}));