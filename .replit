run = "node main.js"
entrypoint = "main.js"
modules = ["nodejs-20", "python-3.11", "python3", "postgresql-16"]

[nix]
channel = "stable-22_11"
packages = ["freetype", "gh", "git-lfs", "jq", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "nano", "openjpeg", "tcl", "tk", "zlib"]

[deployment]
run = "node main.js"
deploymentTarget = "cloudrun"

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 3002
externalPort = 80

[[ports]]
localPort = 8080
externalPort = 8080

[agent]
integrations = ["perplexity_v0==1.0.0", "javascript_websocket==1.0.0", "javascript_stripe==1.0.0", "javascript_openai==1.0.0", "javascript_anthropic==1.0.0"]

[objectStorage]
defaultBucketID = "replit-objstore-f95b4cf3-0db7-40d1-9680-54f5b997ed65"
