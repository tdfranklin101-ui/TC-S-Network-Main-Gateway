run = "node main.js"
entrypoint = "main.js"
modules = ["nodejs-20", "python-3.11", "postgresql-16"]

[nix]
channel = "stable-22_11"
packages = ["freetype", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "tcl", "tk", "zlib"]

[deployment]
run = "node main.js"
deploymentTarget = "cloudrun"

[[ports]]
localPort = 3000
externalPort = 80

[[ports]]
localPort = 3001
externalPort = 3003

[[ports]]
localPort = 3333
externalPort = 3000

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 5001
externalPort = 3001

[[ports]]
localPort = 5002
externalPort = 3002

[[ports]]
localPort = 8080
externalPort = 8080

[agent]
integrations = ["perplexity_v0==1.0.0", "javascript_websocket==1.0.0", "javascript_stripe==1.0.0", "javascript_openai==1.0.0", "javascript_anthropic==1.0.0"]
