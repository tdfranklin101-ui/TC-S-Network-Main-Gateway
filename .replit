run = "node main.js"
entrypoint = "main.js"
modules = ["nodejs-20", "python-3.11", "postgresql-16", "python3"]

[nix]
channel = "stable-22_11"
packages = ["freetype", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "tcl", "tk", "zlib"]

[deployment]
run = "node main.js"
deploymentTarget = "cloudrun"

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 3002
externalPort = 80

