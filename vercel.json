{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "buildCommand": null,
  "outputDirectory": "public",
  "rewrites": [
    { "source": "/health", "destination": "https://current-see-website-tdfranklin101.replit.app/health" },
    { "source": "/api/:path*", "destination": "https://current-see-website-tdfranklin101.replit.app/api/:path*" },
    { "source": "/uploads/:path*", "destination": "https://current-see-website-tdfranklin101.replit.app/uploads/:path*" },
    { "source": "/public/media/:path*", "destination": "https://current-see-website-tdfranklin101.replit.app/public/media/:path*" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    },
    {
      "source": "/uploads/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
