[deployment]
run = "node pure-deployment.js"
entrypoint = "pure-deployment.js"

[Start Production Server]
run = "CURRENTSEE_DB_URL=${CURRENTSEE_DB_URL:-$DATABASE_URL} node pure-deployment.js"
persistent = true
persistent-folder = ["."]

[Check Database Connection]
run = "node check-currentsee-db.js"